{
  "name": "xui-terms-and-conditions",
  "version": "0.0.1",
  "description": "Terms and Conditions Service",
  "main": "index.js",
  "repository": "git@github.com:hmcts/rpx-xui-terms-and-conditions.git",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon -r dotenv/config server/index.ts | pino-pretty",
    "dev:start": "node -r dotenv/config dist/index.js",
    "dev:debug": "nodemon --exec 'node -r ts-node/register --inspect-brk' server/index.ts | pino-pretty",
    "compile": "ts-node build.ts && tsc",
    "create:schema": "yarn compile && node -r dotenv/config dist/database/scripts/create_db.js",
    "create:schema:dev": "ts-node -r dotenv/config server/database/scripts/create_db.ts",
    "start": "node dist/index.js",
    "migrate": "node dist/database/scripts/migrate.js",
    "dev:migrate": "ts-node -r dotenv/config server/database/scripts/migrate.ts",
    "docker:dev": "nodemon -e ts server --exec \"yarn docker:compile\"",
    "docker:compile": "yarn compile && yarn migrate && yarn docker:start",
    "docker:start": "node -r source-map-support/register dist/index.js | pino-pretty",
    "test": "UNIT_TEST_ENVIRONMENT=true jest",
    "test:debug": "mocha --inspect-brk test/**/*.ts --exit",
    "test:coverage": "UNIT_TEST_ENVIRONMENT=true jest --coverage",
    "test:watch": "UNIT_TEST_ENVIRONMENT=true jest --watch",
    "test:a11y": "echo 'not implemented'",
    "test:smoke": "yarn migrate",
    "test:functional": "echo 'not implemented'",
    "sonar-scan": "node_modules/sonar-scanner/bin/sonar-scanner",
    "lint": "eslint server/api/**/*.ts server/database/**/*.ts",
    "lint:fix": "prettier eslint server/api/**/*.ts server/database/**/*.ts --write",
    "lint:legacy": "eslint .",
    "start:legacy": "node server.js",
    "test:legacy": "NODE_PATH=. LOG_LEVEL=OFF mocha"
  },
  "wallaby": {
    "env": {
      "params": {
        "env": "UNIT_TEST_ENVIRONMENT=true"
      }
    }
  },
  "dependencies": {
    "@hmcts/properties-volume": "^0.0.9",
    "applicationinsights": "^1.6.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "config": "^3.2.4",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "global-agent": "^2.1.7",
    "global-tunnel-ng": "^2.7.1",
    "jwt-decode": "^2.2.0",
    "otp": "^0.1.3",
    "pg-monitor": "^1.3.1",
    "pg-promise": "^9.3.3",
    "pino": "^5.13.4",
    "postgres-migrations": "^4.0.0-beta.1",
    "sonar-scanner": "^3.1.0",
    "swagger-express-middleware": "^2.0.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/config": "^0.0.36",
    "@types/express": "^4.17.1",
    "@types/global-tunnel-ng": "^2.1.0",
    "@types/jest": "^24.0.19",
    "@types/pino": "^5.8.10",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "chai": "^4.2.0",
    "dotenv": "^8.1.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "mocha": "^6.2.1",
    "nodemon": "^1.19.3",
    "pino-pretty": "^3.2.1",
    "prettier": "^1.18.2",
    "shelljs": "^0.8.3",
    "sinon": "^7.3.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-lint": "^4.5.1",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  }
}
